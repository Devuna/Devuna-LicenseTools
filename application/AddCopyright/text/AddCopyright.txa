EMBED %ProcedureRoutines
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
ProcessCopyright  ROUTINE
   FREE(CopyrightQueue)
   TextFilename = CopyrightFilename
   OPEN(TextFile)
   SET(TextFile)
   NEXT(TextFile)
   LOOP UNTIL ERRORCODE()
      !Replace CopyrightDate token
      CopyrightQueue.CopyrightText = ''
      I = 1
      J = INSTRING('%CopyrightDate',TextFile.Line,1,I)
      LOOP WHILE J
          CopyrightQueue.CopyrightText = CopyrightQueue.CopyrightText & TextFile.Line[I : J-1] & CopyrightDate
          I = J + 14
          J = INSTRING('%CopyrightDate',TextFile.Line,1,I)
      END
      CopyrightQueue.CopyrightText = CopyrightQueue.CopyrightText & TextFile.Line[I : LEN(CLIP(TextFile.Line))]
      TextFile.Line = CopyrightQueue.CopyrightText
      
      !Replace CopyrightOwner token
      CopyrightQueue.CopyrightText = ''
      I = 1
      J = INSTRING('%CopyrightOwner',TextFile.Line,1,I)
      LOOP WHILE J
          CopyrightQueue.CopyrightText = CopyrightQueue.CopyrightText & TextFile.Line[I : J-1] & CopyrightOwner
          I = J + 15
          J = INSTRING('%CopyrightOwner',TextFile.Line,1,I)
      END
      CopyrightQueue.CopyrightText = CopyrightQueue.CopyrightText & TextFile.Line[I : LEN(CLIP(TextFile.Line))]
      TextFile.Line = CopyrightQueue.CopyrightText
      
      !Replace Repository token
      CopyrightQueue.CopyrightText = ''
      I = 1
      J = INSTRING('%Repository',TextFile.Line,1,I)
      LOOP WHILE J
          CopyrightQueue.CopyrightText = CopyrightQueue.CopyrightText & TextFile.Line[I : J-1] & Repository
          I = J + 11
          J = INSTRING('%Repository',TextFile.Line,1,I)
      END
      CopyrightQueue.CopyrightText = CopyrightQueue.CopyrightText & TextFile.Line[I : LEN(CLIP(TextFile.Line))]

      ADD(CopyrightQueue)
      NEXT(TextFile)
   END
   CLOSE(TextFile)
   EXIT
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
InjectCopyright   ROUTINE
   LOOP p = 1 TO RECORDS(CopyrightQueue)
      GET(CopyrightQueue,p)
      TextOutFile.Line = CopyrightQueue.CopyrightText
      ADD(TextOutFile)
   END   
   EXIT
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
ProcessFile  ROUTINE
   CREATE(TextOutFile)
   IF NOT ERRORCODE()
      OPEN(TextOutFile)
      IF NOT ERRORCODE()     
         OPEN(TextFile)
         IF NOT ERRORCODE()
            DO InjectCopyright
            SET(TextFile)
            NEXT(TextFile)
            LOOP UNTIL ERRORCODE()
               TextOutFile.Line = TextFile.Line
               ADD(TextOutFile)
               NEXT(TextFile)
            END
            CLOSE(TextFile)
            CLOSE(TextOutFile)
         ELSE
            CLOSE(TextOutFile)
            REMOVE(TextOutFile)
         END
      END
   END
   EXIT
[END]
