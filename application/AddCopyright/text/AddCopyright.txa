[EMBED]
EMBED %ControlEventHandling
[INSTANCES]
WHEN '?CloseButton'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
POST(EVENT:CloseWindow)
[END]
[END]
WHEN '?OkButton'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
!Process the Copyright file
DO ProcessCopyright

!Process selected files

LOOP n = 1 TO RECORDS(Source_Queue)
   GET(Source_Queue,n)
   IF Source_Queue.Selected = TRUE
      TextFilename = Source_Folder & '\' & CLIP(Source_Queue.Name)
      TextOutFilename = Target_Folder & '\' & CLIP(Source_Queue.Name)
      DO ProcessFile
   END
END

SELECT(?CloseButton)
[END]
[END]
WHEN '?LookupSourceFolder'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 7501
PROPERTY:END
SELF.Update()
Target_Folder = Source_Folder & '\updated'
DISPLAY(?Target_Folder)
FREE(Source_Queue)
!Get directory list of files
DIRECTORY(Source_Queue,Source_Folder & '\*.*',ff_:NORMAL)
LOOP I = 1 TO RECORDS(Source_Queue)
   GET(Source_Queue,I)
   Source_Queue.Selected = FALSE
   Source_Queue.Icon = 1
   PUT(Source_Queue)
END
SORT(Source_Queue,+Source_Queue.Name)
GET(Source_Queue,1)
?FileList{PROP:Selected} = 1
DISPLAY(?FileList)
[END]
[END]
WHEN '?SelectButton'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
GET(Source_Queue,?FileList{PROP:Selected})
Source_Queue.Selected = ABS(Source_Queue.Selected - 1)
IF Source_Queue.Selected = TRUE
   SelectedCount += 1
ELSE
   SelectedCount -= 1
END
Source_Queue.Icon = Source_Queue.Selected + 1
PUT(Source_Queue) 

IF ?FileList{PROP:Selected} = RECORDS(Source_Queue)
   GET(Source_Queue,1)
   ?FileList{PROP:Selected} = 1
ELSE   
   GET(Source_Queue,POINTER(Source_Queue) + 1)
   ?FileList{PROP:Selected} = ?FileList{PROP:Selected} + 1
END   

DISPLAY(?FileList)
SELF.Update()
!POST(EVENT:NewSelection,?FileList) 
[END]
[END]
WHEN '?SelectAllButton'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
SelectedCount = RECORDS(Source_Queue)
LOOP I = 1 TO SelectedCount
   GET(Source_Queue,I)
   Source_Queue.Selected = TRUE
   Source_Queue.Icon = 2
   PUT(Source_Queue) 
END
SELF.Update()
[END]
[END]
WHEN '?FileList'
[INSTANCES]
WHEN 'NewSelection'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
GET(Source_Queue,CHOICE(?FileList))
IF Source_Queue.Selected
   ?SelectButton{PROP:Text} = 'De&select'
ELSE
   ?SelectButton{PROP:Text} = '&Select'
END   
